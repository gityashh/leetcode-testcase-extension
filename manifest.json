{
    "manifest_version": 3,
    "name": "LeetCode Test Case Helper",
    "version": "1.0",
    "description": "Provides example test cases on LeetCode problem pages.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://leetcode.com/problems/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }